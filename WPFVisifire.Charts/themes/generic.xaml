                                                <StackPanel Name="RightAxisPanel" />
                                            </StackPanel>
                                        </Grid>
                                        <Grid Name="BottomAxisGrid" Grid.Row="2" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Stretch">
                                            <StackPanel Name="BottomAxisContainer" Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                                                <zb:ZoomBar x:Name="BottomAxisScrollBar" MinHeight="0" Height="17" Orientation="Horizontal" Visibility="Collapsed" />
                                                <StackPanel Name="BottomAxisPanel" />
                                            </StackPanel>
                                        </Grid>
