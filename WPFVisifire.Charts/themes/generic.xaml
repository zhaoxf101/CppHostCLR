                                            <Grid Name="PlotAreaGrid" ZIndex="0" Margin="0,0,0,0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                                                <Canvas Name="PlotAreaScrollViewer" Margin="0">
                                                    <Canvas Name="PlotCanvas">
                                                        <Canvas Name="DrawingCanvas" ZIndex="1" Margin="0,0,0,0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
                                                    </Canvas>
                                                </Canvas>
                                            </Grid>
                                            <Grid Name="InnerGrid" ZIndex="1" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="Auto" />
                                                    <ColumnDefinition />
                                                    <ColumnDefinition Width="Auto" />
                                                </Grid.ColumnDefinitions>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="Auto" />
                                                    <RowDefinition />
                                                    <RowDefinition Height="Auto" />
                                                </Grid.RowDefinitions>
                                                <StackPanel Name="TopInnerPanel" Grid.Row="0" Grid.ColumnSpan="3">
                                                    <StackPanel Name="TopInnerTitlePanel" />
                                                    <StackPanel Name="TopInnerLegendPanel" />
                                                </StackPanel>
                                                <StackPanel Name="BottomInnerPanel" Grid.Row="2" Grid.ColumnSpan="3">
                                                    <StackPanel Name="BottomInnerLegendPanel" />
                                                    <StackPanel Name="BottomInnerTitlePanel" />
                                                </StackPanel>
                                                <StackPanel Name="LeftInnerPanel" Grid.Column="0" Grid.Row="1" Orientation="Horizontal">
                                                    <StackPanel Name="LeftInnerTitlePanel" Orientation="Horizontal" />
                                                    <StackPanel Name="LeftInnerLegendPanel" Orientation="Horizontal" />
                                                </StackPanel>
                                                <StackPanel Name="RightInnerPanel" Grid.Column="2" Grid.Row="1" Orientation="Horizontal">
                                                    <StackPanel Name="RightInnerLegendPanel" Orientation="Horizontal" />
                                                    <StackPanel Name="RightInnerTitlePanel" Orientation="Horizontal" />
                                                </StackPanel>
                                            </Grid>
                                        </Grid>
                                        <Canvas Name="ZoomAreaCanvas" ZIndex="2" Margin="0">
                                            <Border Name="ZoomRectangle" CornerRadius="0" BorderBrush="#FF000000" Background="#FFD3D3D3" Opacity="0.4" Panel.ZIndex="2" />
                                        </Canvas>
                                    </Grid>
                                </Grid>
                            </Grid>
                        </Border>
                        <Canvas Name="BevelCanvas" ZIndex="2" Margin="0,0,0,0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
                        <StackPanel Name="CenterDockInsidePlotAreaPanel" ZIndex="3" HorizontalAlignment="Center" VerticalAlignment="Center" />
                        <StackPanel Name="CenterDockOutsidePlotAreaPanel" ZIndex="4" HorizontalAlignment="Center" VerticalAlignment="Center" />
                        <Canvas Name="ToolTipCanvas" ZIndex="5" Height="{TemplateBinding FrameworkElement.Height}" Width="{TemplateBinding FrameworkElement.Width}" Margin="0,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" />
                        <Canvas Name="ElementCanvas" ZIndex="5" />
                        <Canvas Name="ControlsHolder" Height="0" Width="0" />
                        <Border Name="LicenseInfoElement" Visibility="Collapsed" HorizontalAlignment="Stretch" VerticalAlignment="Top" Height="20" Background="#FFCA5100" Panel.ZIndex="91000">
                            <TextBlock Text="You are using the trial version" TextBlock.FontSize="14" TextBlock.Foreground="#FFFFFFFF" HorizontalAlignment="Center" />
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Control.Padding" Value="6" />
        <Setter Property="vc:Chart.Background" Value="#00FFFFFF" />
        <Setter Property="vc:Chart.BorderThickness" Value="0.5" />
        <Setter Property="vcm:VisifireControl.ToolTipEnabled" Value="True" />
        <Setter Property="vc:Chart.ColorSet" Value="Visifire1" />
    </Style>
    <Style x:Key="{x:Type vc:ToolTip}" TargetType="{x:Type vc:ToolTip}">
        <Setter Property="UIElement.IsHitTestVisible" Value="False" />
        <Setter Property="UIElement.Visibility" Value="Collapsed" />
        <Setter Property="FrameworkElement.HorizontalAlignment" Value="Left" />
        <Setter Property="vc:ToolTip.CornerRadius" Value="4,4,4,4" />
        <Setter Property="Control.BorderThickness" Value="0.25,0.25,0.25,1" />
        <Setter Property="Control.BorderBrush" Value="#FF000000" />
        <Setter Property="Control.Padding" Value="10,5,10,3" />
        <Setter Property="Control.FontFamily" Value="Portable User Interface" />
        <Setter Property="Control.FontSize" Value="12" />
        <Setter Property="Control.Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type vc:ToolTip}">
                    <Grid IsHitTestVisible="False">
                        <Canvas Margin="0" ZIndex="2">
                            <Path Name="CallOut" StrokeThickness="0" Stroke="#FF000000" Fill="#FFD3D3D3">
                                <Path.Data>
                                    <PathGeometry>
                                        <PathFigure x:Name="CallOutPathFigure" IsClosed="False" StartPoint="0,6">
                                            <LineSegment x:Name="CallOutLineSegment1" Point="-15,15" />
                                            <LineSegment x:Name="CallOutLineSegment2" Point="0,20" />
                                        </PathFigure>
                                    </PathGeometry>
                                </Path.Data>
                            </Path>
                        </Canvas>
                        <Border Name="ToolTipBorder" IsHitTestVisible="False" Visibility="{TemplateBinding UIElement.Visibility}" CornerRadius="{TemplateBinding vc:ToolTip.CornerRadius}" BorderBrush="{TemplateBinding Control.BorderBrush}" BorderThickness="{TemplateBinding Control.BorderThickness}" Padding="{TemplateBinding Control.Padding}" Background="{TemplateBinding vc:ToolTip.InternalBackground}">
                            <Border.Child>
                                <TextBlock Name="ToolTipTextBlock" TextBlock.LineHeight="{TemplateBinding Control.FontSize}" TextBlock.LineStackingStrategy="BlockLineHeight" TextWrapping="Wrap" Text="{TemplateBinding vc:ToolTip.Text}" HorizontalAlignment="Center" VerticalAlignment="Center" TextBlock.FontFamily="{TemplateBinding Control.FontFamily}" TextBlock.FontSize="{TemplateBinding Control.FontSize}" TextBlock.FontWeight="{TemplateBinding Control.FontWeight}" TextBlock.FontStyle="{TemplateBinding Control.FontStyle}" />
                            </Border.Child>
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
