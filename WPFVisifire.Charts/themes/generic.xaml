                                            </StackPanel>
                                        </Grid>
                                        <Grid Name="LeftAxisGrid" Grid.Row="1" Grid.RowSpan="2" Grid.Column="0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                                            <StackPanel Name="LeftAxisContainer" Orientation="Horizontal" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                                                <StackPanel Name="LeftAxisPanel" />
                                                <zb:ZoomBar x:Name="LeftAxisScrollBar" MinWidth="0" Width="17" VerticalAlignment="Top" Orientation="Vertical" Visibility="Collapsed" />
                                            </StackPanel>
                                        </Grid>
                                        <Grid Name="RightAxisGrid" Grid.Row="1" Grid.RowSpan="2" Grid.Column="2" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                                            <StackPanel Name="RightAxisContainer" Orientation="Horizontal" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
